{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "includeFiles": [
        "prompts/**"
      ]
    }
  },
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
